[core]
	quotepath = false
	excludesfile = v:/home/.gitignore
[guitool "reset/file"]
	cmd = git checkout $FILENAME
	noconsole = yes
	needsfile = yes
[guitool "Add Selected file to .gitignore"]
	cmd = echo $FILENAME >> .gitignore
	needsfile = yes
[guitool "add text into .gitignore"]
	cmd = echo \"$ARGS\" | tee -a  .gitignore
	argprompt = yes
[gui]
