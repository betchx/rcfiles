	name = Atsushi TANABE
[http]
	proxy = ocp16:8080
#	proxy = 172.16.0.56:8080
#	proxy = oproxy:8080   # old one
[guitool "set_file_encoding"]
	cmd = echo $FILENAME encoding=$ARGS >> .gitattributes
	needsfile = yes
	argprompt = yes
